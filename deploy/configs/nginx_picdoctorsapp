server {
    listen 80;
    server_name picdoctors.com;
    server_name www.picdoctors.com;
    root /code/picdoctors;

    location /static/ {
        alias /code/picdoctors/static/;
        #include /etc/nginx/gzip.conf;
        expires 30d;
        access_log off;
    }

    location / {
        include uwsgi_params;
        uwsgi_pass unix:/tmp/picdoctorsapp.sock;
    }
}

